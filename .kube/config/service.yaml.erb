---
apiVersion: v1
kind: Service
metadata:
  labels:
    app: <%= namespace %>
    krane-prune: "true"
  name: web
  namespace: <%= namespace %> 
  annotations:
    cloud.google.com/app-protocols: '{"https":"HTTPS"}'
    cloud.google.com/neg: '{"ingress": true}'
spec:
  ports:
  - name: http
    port: 80
    protocol: TCP
    targetPort: 8080
  selector:
    app: <%= namespace %>
  type: NodePort
